<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title></title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

</head>
<body>
	<div class="container">
		<table class="table table-bordered table-border text-center shadow">
			<tr>

				<th>book id</th>
				<th>book name</th>
				<th>issue date</th>
				<th>book delete</th>
				<th>book edit</th>
		    </tr>
		    <% users.forEach(function(user){  %>
		    <tr>
		    	<td><%= user.book_id %></td>
		    	<td><%= user.book_name %></td>
		    	<td><%= user.issue_date %></td>
		    	<td><a href="/delete/<%= user.book_id %>" class="btn btn-danger btn-sm">delete</a></td>
		    	<td><a href="/edit/<%= user.book_id %>" class="btn btn-danger btn-sm">edit</a></td>

		    	
		    </tr>
		    <% }) %>


			

		</table>
	</div>

</body>
</html>